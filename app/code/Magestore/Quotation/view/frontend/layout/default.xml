<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="header.links">
            <block class="Magento\Framework\View\Element\Html\Link\Current"
                   name="quotation-cart-header-links"
                   after="register-link-new"
                   ifconfig="quotation/general/enable"
                   cacheable="false" >
                <arguments>
                    <argument name="label" xsi:type="string" translate="true">Quote Me</argument>
                    <argument name="path" xsi:type="string">quotation/quote</argument>
                </arguments>
            </block>
        </referenceBlock>
        <referenceBlock name="top.links">
            <block class="Magento\Framework\View\Element\Html\Link\Current"
                   name="quotation-cart-top-links"
                   after="register-link-new"
                   ifconfig="quotation/general/enable"
                   cacheable="false" >
                <arguments>
                    <argument name="label" xsi:type="string" translate="true">Quote Me</argument>
                    <argument name="path" xsi:type="string">quotation/quote</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
