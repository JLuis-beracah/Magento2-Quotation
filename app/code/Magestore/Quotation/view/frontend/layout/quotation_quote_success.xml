<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Success Page</title>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Thank you for your request!</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magestore\Quotation\Block\Cart\Success" name="quotation.success" template="Magestore_Quotation::cart/success.phtml" cacheable="false">
            </block>
        </referenceContainer>
    </body>
</page>
